{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@app/ts/web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["unplugin-vue-define-options/macros-global"],
    "moduleResolution": "node"
  },
  "vueCompilerOptions": {
    "target": 3,
    "plugins": ["@vue-macros/volar/define-options"]
  },
  "include": ["src"]
}
